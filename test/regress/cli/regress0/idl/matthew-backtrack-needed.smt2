(set-option :produce-models true)
(set-logic QF_LIA)
(set-info :source |Tests out a variety of syntaxes that the rewriter should support
by Matthew Sotoudeh
|)
(set-info :smt-lib-version 2.0)
(set-info :category "crafted")
(set-info :status sat)
(declare-const x Int)
(declare-const y Int)
(declare-const z Int)
(assert (and (or (> x y) (> x z))
            (or  (< x y) (< x z) )))
(check-sat)
(get-model)
